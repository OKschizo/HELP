# Deployment Configuration
# Copy this file to .env and fill in your values

# CRITICAL: Never commit your private key to version control
# Use a dedicated deployment wallet with minimal funds
DEPLOYER_KEY=your_private_key_here_without_0x_prefix

# Network RPC URLs (optional - defaults are provided in hardhat.config.cjs)
# HYPEREVM_RPC_URL=https://rpc.hyperliquid.xyz/evm
# HYPEREVM_TESTNET_RPC_URL=https://rpc.hyperliquid-testnet.xyz/evm

# Etherscan API Key for contract verification (optional)
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Platform Configuration (optional - can also be set in deploy-config.json)
PLATFORM_FEE_RECEIVER=0x0000000000000000000000000000000000000000
PLATFORM_FEE_BPS=250
